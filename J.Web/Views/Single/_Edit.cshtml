@{
    Layout = null;
}

<!-- Edit -->
<div class="modal fade" id="EditDialog" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h2 class="modal-title">详细</h2>
            </div>
            <form id="EditDialog_form">
                <div class="modal-body form-horizontal">
                    <div class="form-group">
                        <label for="EditDialog_GUID" class="col-sm-3 control-label">GUID</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_GUID"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleIntNumber" class="col-sm-3 control-label">SingleIntNumber</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleIntNumber"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleIntEnum" class="col-sm-3 control-label">SingleIntEnum</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleIntEnum"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleMoney" class="col-sm-3 control-label">SingleMoney</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleMoney"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleDatetime" class="col-sm-3 control-label">SingleDatetime</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleDatetime"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleVarchar" class="col-sm-3 control-label">SingleVarchar</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleVarchar"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleLongVarchar" class="col-sm-3 control-label">SingleLongVarchar</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleLongVarchar">

                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="EditDialog_SingleBit" class="col-sm-3 control-label">SingleBit</label>
                        <div class="col-sm-9">
                            <p class="form-control-static" id="EditDialog_SingleBit">
                                <i class="fa fa-check"></i><i class="fa fa-times"></i>
                            </p>
                        </div>
                    </div>
                    <div class="alert alert-danger alert-dismissable hidden" id="EditDialog_alert">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <p id="EditDialog_alert_message"></p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="EditDialog_Close" class="btn btn-lg btn-warning" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script type="text/javascript">
    {
        var Common_User_Edit_url = "@Url.Action("Edit", "User", new { area = "Common" }, null)";
    }

    $(function () {
    });
</script>